<% if(messages.errors){ %>
    <div class="alert alert-danger alert-dismissible fade show" style="text-align: center;display:flex;" role="alert">
        <%= messages.errors %>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
<% }%>

